{
  "expo": {
    "name": "KidsPrize",
    "description": "Kids behaviour chart",
    "slug": "kidsprize",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "version": "1.4.3",
    "githubUrl": "https://github.com/ericvan76/KidsPrizeMobile",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "packagerOpts": {
      "sourceExts": [
        "ts",
        "tsx"
      ],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff"
    },
    "scheme": "kidsprize",
    "ios": {
      "bundleIdentifier": "com.kids-prize.mobile.KidsPrize",
      "buildNumber": "1040310",
      "supportsTablet": false,
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {},
      "loadJSInBackgroundExperimental": true
    },
    "android": {
      "package": "com.kidsprize",
      "versionCode": 1040310,
      "permissions": [
        "USE_FINGERPRINT"
      ]
    },
    "extra": {
      "semver": "1.4.3-beta.10",
      "apiEndpoint": "https://dev-01.fanstek.com/v2",
      "auth0Domain": "apperic.auth0.com",
      "auth0ClientId": "<auth0-client-id>",
      "auth0ClientSecret": "<auth0-client-secret>"
    }
  }
}
